{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "grafana"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "title": "API Requests per Second",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job='api'}[5m]))",
          "legendFormat": "{{method}} {{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {"unit": "short"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "title": "Worker Queue Depth",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(celery_tasks_in_queue{queue=~'default|training'})",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {"unit": "short"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "title": "Model Training Jobs Status",
      "type": "stat",
      "options": {
        "reduceOptions": {
          "calcs": ["last"],
          "fields": "",
          "values": false
        },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "sum(rate(automl_training_jobs_total[5m])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 4,
      "title": "API Resource Utilization",
      "type": "timeseries",
      "targets": [
        {
          "expr": "avg(rate(container_cpu_usage_seconds_total{container='automl_api'}[5m])) * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        },
        {
          "expr": "avg(container_memory_usage_bytes{container='automl_api'}) / 1024 / 1024",
          "legendFormat": "Memory MB",
          "refId": "B"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "id": 5,
      "title": "Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job='api',status=~'5..'}[5m]))",
          "legendFormat": "5xx",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_requests_total{job='api',status=~'4..'}[5m]))",
          "legendFormat": "4xx",
          "refId": "B"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "id": 6,
      "title": "MinIO Storage Utilization",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(minio_bucket_object_size_bytes{bucket=~'models|datasets|artifacts'}) / 1024 / 1024 / 1024",
          "legendFormat": "{{bucket}}",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["automl", "operations"],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"},
